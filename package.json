{
  "name": "agfit",
  "version": "1.0.0",
  "description": "AI-Powered Health & Wellness Platform",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm run dev",
    "build": "npm run build:frontend",
    "build:frontend": "cd frontend && npm run build",
    "build:backend": "cd backend && npm install",
    "test": "npm run test:backend && npm run test:frontend",
    "test:backend": "cd backend && npm test",
    "test:frontend": "cd frontend && npm test",
    "install:all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "clean": "rm -rf node_modules backend/node_modules frontend/node_modules frontend/dist",
    "start": "cd backend && npm start"
  },
  "keywords": [
    "health",
    "wellness",
    "fitness",
    "ai",
    "tracking",
    "nutrition",
    "workout"
  ],
  "author": "AgFit Team",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2",
    "axios": "^1.6.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/agfit.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/agfit/issues"
  },
  "homepage": "https://yourusername.github.io/agfit/"
}
